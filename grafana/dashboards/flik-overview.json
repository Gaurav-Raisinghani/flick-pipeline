  "panels": [
    {
      "title": "Queue Depth by Priority",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "flik_queue_depth",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 20, "lineWidth": 2 },
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "Throughput (tasks/sec)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(flik_tasks_submitted_total[1m])",
          "legendFormat": "Submitted ({{type}})",
          "refId": "A"
        },
        {
          "expr": "rate(flik_tasks_completed_total[1m])",
          "legendFormat": "Completed ({{type}} - {{status}})",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 10, "lineWidth": 2 },
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "title": "Processing Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(flik_task_processing_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(flik_task_processing_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(flik_task_processing_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Failure Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(rate(flik_tasks_completed_total{status=\"FAILED\"}[5m])) / sum(rate(flik_tasks_completed_total[5m])) * 100",
          "legendFormat": "Failure %",
          "refId": "A"
        },
        {
          "expr": "rate(flik_tasks_completed_total{status=\"DEAD_LETTERED\"}[5m])",
          "legendFormat": "Dead-lettered/sec",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 20, "lineWidth": 2 },
          "unit": "percent"
        },
        "overrides": []
      }
    },
